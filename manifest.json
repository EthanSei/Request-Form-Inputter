{
    "manifest_version": 2,
    "name": "Requisition",
    "version": "1.0",
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "content_scripts":[{
        "js":["content.js"],
        "matches":["<all_urls>"]
    }]
}